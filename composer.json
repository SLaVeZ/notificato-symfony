{
	"name": "wrep/notificato-symfony",
	"description": "Takes care of push notifications in your Symfony2 projects.",
	"type": "symfony-bundle",
	"keywords": ["push", "notification", "ios", "apns", "symfony2"],
	"homepage": "https://github.com/wrep/notificato-symfony",
	"license": "MIT",
	"authors": [
		{
			"name": "Rick Pastoor",
			"email": "rick@wrep.nl"
		},
		{
			"name": "Mathijs Kadijk",
			"email": "mathijs@wrep.nl"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"symfony/symfony": "2.*",
		"wrep/notificato": "0.5.*"
	},
	"replace": {
		"wrep/notificato-notificare": "*"
	},
	"autoload": {
		"psr-0": { "Wrep\\Bundle\\NotificatoBundle": "" }
	},
	"target-dir": "Wrep/Bundle/NotificatoBundle"
}
