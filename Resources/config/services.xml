<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
    	<parameter key="notificare.apns.certificate.pem"></parameter>
    	<parameter key="notificare.apns.certificate.passphrase"></parameter>
        <parameter key="notificare.sender.class">Wrep\Bundle\NotificareBundle\Notificare\Sender</parameter>
    </parameters>

    <services>
        <service id="notificare.sender" class="%notificare.sender.class%">
            <argument type="service" id="service_container" />
        </service>
    </services>
</container>
